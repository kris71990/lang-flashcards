{
  "name": "front-end",
  "version": "1.0.0",
  "description": "react frontend to language cards",
  "main": "index.js",
  "scripts": {
    "prestart": "npm run build",
    "start": "node index.js",
    "test": "eslint . && jest --coverage",
    "test-nolint": "jest --coverage",
    "build": "webpack --config webpack.prod.js",
    "watch": "webpack-dev-server --config webpack.dev.js"
